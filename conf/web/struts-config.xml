<struts-config>

  <!-- ========== Form Bean Definitions =================================== -->
  <form-beans>

    <form-bean name="freestyleForm" type="gov.nih.nci.cadsr.freestylesearch.tool.FreestyleSearchForm"/>

  </form-beans>
  
  <!-- ========== Action Mapping Definitions ============================== -->
  <action-mappings>

    <action path="/search"
            type="gov.nih.nci.cadsr.freestylesearch.tool.FreestyleSearch"
            name="freestyleForm"
            scope="request"
            input="/jsp/freestylesearch.jsp">
    </action>

  </action-mappings>
  
  <message-resources
    null="false"
    parameter="gov.nih.nci.cadsr.freestylesearch.tool.Freestyle"/>
    
  <plug-in className="gov.nih.nci.cadsr.freestylesearch.tool.FreestylePlugIn" />

</struts-config>
