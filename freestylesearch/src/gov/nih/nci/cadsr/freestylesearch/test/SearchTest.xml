        <!-- Define the types of data returns:
            0 = default
            1 = abbreviated (idseq, score, type)
            2 = caCORE API Administered Component Objects
            3 = a Generic AC Result Set
        -->
        <entry key="returns">1</entry>

        <!-- Specify the match test type, 0 = exact, 1 = partial, 2 = best.
        -->
        <entry key="matchType">2</entry>
    
        <!-- Specify the maximum possible number of returns.
        -->
        <entry key="limit">50</entry>
    
        <!-- Specify the maximum number of scores, e.g. 3 is the top 3 scores which may be 56, 23, 2.
        -->
        <entry key="scores">3</entry>
    
        <!-- Specify the return types of the results. If all are desired comment out this block.
               0 = DE, 1 = DEC, 2 = VD, etc.
        <entry key="restricts">0, 1, 2</entry>
        -->
        
        <!-- Restrict the results to non-retired Workflow Status
        -->
        <entry key="restrictResultsByWorkflowNotRetired">false</entry>
    
